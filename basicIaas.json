  "contentVersion": "1.0.0.0",
  "parameters": {
    "virtualMachineSize": {
        "type": "string",
        "defaultValue": "Basic_A2",
        "metadata": {
            "description": "Virtual machine size (has to be at least the size of Standard_A3 to support 2 NICs)"
        }
